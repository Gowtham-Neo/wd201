<table>
    <tr>
      <th>TITLE</th>
      <th>DUE DATE</th>
      <th>COMPLETED</th>
    </tr>
    <% for(var i=0;i< allTodos.length;i++) { %>
      <tr>
        <td>
          <%= allTodos[i].title %>
        </td>
        <td>
          <%= allTodos[i].dueDate %>
        </td>
        <td>
          <%= allTodos[i].completed %>
        </td>
      </tr>
    <% } %>


  </table>











